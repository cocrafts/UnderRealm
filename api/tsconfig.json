{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2017",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "types/*": ["./types/*"],
      "resolvers/*": ["./resolvers/*"],
      "aws/*": ["./aws/*"]
    }
  },
  "exclude": ["node_modules"]
}
