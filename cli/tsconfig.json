{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es2017",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "paths": {
      "utils/*": ["./utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
